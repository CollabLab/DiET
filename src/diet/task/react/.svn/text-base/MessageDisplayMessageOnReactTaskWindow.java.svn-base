package diet.task.react;

import diet.message.MessageTask;

/**
 *
 * @author sre
 */
public class MessageDisplayMessageOnReactTaskWindow extends MessageTask{

    String text;
    boolean showGrid;

    public MessageDisplayMessageOnReactTaskWindow(String email, String username, String text,boolean showGrid) {
       super(email,username);
       this.text=text;
       this.showGrid=showGrid;
    }

    public boolean getShowGrid() {
        return showGrid;
    }

    public String getText() {
        return text;
    }


}
